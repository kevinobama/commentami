{
  "name": "comments-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "lodash": "^4.17.5",
    "react": "^16.3.0",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.3.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "format": "prettier-eslint \"client/**/*.js\" --write --singleQuote --bracketSpacing --no-semi",
    "test": "",
    "ci": "CI=true npm run test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint client webpack.config.js",
    "precommit": "lint-staged && npm run lint && npm run ci"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write --singleQuote --bracketSpacing",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "no-console": [
        "warn"
      ],
      "jsx-a11y/href-no-hash": "off"
    }
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "autoprefixer": "8.2.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "7.1.4",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "^2.4.1",
    "css-loader": "0.28.11",
    "dotenv": "5.0.1",
    "dotenv-expand": "4.2.0",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-class-property": "^1.1.0",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-loader": "2.1.3",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1",
    "react-hot-loader": "^4.0.0",
    "serverless-stack-output": "^0.2.3",
    "style-loader": "0.20.3",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "1.0.1",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2"
  }
}
